{
  "name": "@kibadist/agentui-nest",
  "version": "0.1.1",
  "description": "AgentUI NestJS module and decorators",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibadist/agentui.git",
    "directory": "packages/nest"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
