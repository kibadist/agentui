  "dependencies": {
    "@kibadist/agentui-protocol": "workspace:*",
    "@kibadist/agentui-validate": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.7.3"
  }
}
