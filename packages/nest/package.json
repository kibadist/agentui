{
  "name": "@agentui/nest",
  "version": "0.1.0",
  "description": "NestJS module for serving AgentUI streams via SSE",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibadist/agentui.git",
    "directory": "packages/nest"
  },
  "homepage": "https://github.com/kibadist/agentui/tree/main/packages/nest#readme",
  "bugs": {
    "url": "https://github.com/kibadist/agentui/issues"
  },
  "keywords": ["agentui", "nestjs", "sse", "ai", "agent", "backend"],
  "dependencies": {
    "@agentui/protocol": "workspace:*",
    "@agentui/validate": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.7.3"
  }
}
