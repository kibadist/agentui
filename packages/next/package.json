{
  "name": "@agentui/next",
  "version": "0.1.0",
  "description": "Next.js API route helpers for proxying AgentUI streams and actions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibadist/agentui.git",
    "directory": "packages/next"
  },
  "homepage": "https://github.com/kibadist/agentui/tree/main/packages/next#readme",
  "bugs": {
    "url": "https://github.com/kibadist/agentui/issues"
  },
  "keywords": ["agentui", "nextjs", "api-routes", "proxy", "sse", "ai"],
  "dependencies": {
    "@agentui/protocol": "workspace:*",
    "@agentui/validate": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
