{
  "name": "@agentui/react",
  "version": "0.1.0",
  "description": "React components and hooks for rendering AgentUI streams",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibadist/agentui.git",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/kibadist/agentui/tree/main/packages/react#readme",
  "bugs": {
    "url": "https://github.com/kibadist/agentui/issues"
  },
  "keywords": ["agentui", "react", "components", "hooks", "ai", "agent", "ui"],
  "dependencies": {
    "@agentui/protocol": "workspace:*",
    "@agentui/validate": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "react": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
