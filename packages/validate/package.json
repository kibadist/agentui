{
  "name": "@agentui/validate",
  "version": "0.1.0",
  "description": "Zod-based validation schemas and parsers for AgentUI events",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibadist/agentui.git",
    "directory": "packages/validate"
  },
  "homepage": "https://github.com/kibadist/agentui/tree/main/packages/validate#readme",
  "bugs": {
    "url": "https://github.com/kibadist/agentui/issues"
  },
  "keywords": ["agentui", "validate", "zod", "schema", "ai", "agent"],
  "dependencies": {
    "@agentui/protocol": "workspace:*",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
